<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mateusz.testmodelkotlin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BrowseTrainingsActivity" />

        <service
            android:name=".TrainingService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".BrowseDaysActivity" />
        <activity android:name=".BrowseWorkoutsActivity" />
        <activity android:name=".BrowseExercisesActivity" />
        <activity android:name=".ExerciseActivity" />
        <activity android:name=".ExerciseInfoActivity" />
        <activity android:name=".ProgressActivity"></activity>
    </application>

</manifest>